{
  "name": "angular-shell-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/angular-shell-e2e/src",
  "tags": [],
  "implicitDependencies": ["angular-shell"],
  "targets": {
    "e2e": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "apps/angular-shell-e2e/cypress.config.ts",
        "testingType": "e2e",
        "devServerTarget": "angular-shell:serve:development"
      },
      "configurations": {
        "production": {
          "devServerTarget": "angular-shell:serve:production"
        },
        "ci": {
          "devServerTarget": "angular-shell:serve-static"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
